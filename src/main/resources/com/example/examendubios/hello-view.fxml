<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="575.0" prefWidth="369.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.examendubios.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label text="Register" />
      <TextField fx:id="numeTextField" prefHeight="26.0" prefWidth="181.0" promptText="nume" />
      <TextField fx:id="prenumeTextField" promptText="prenume" />
      <TextField fx:id="usernameTextField" promptText="username" />
      <TextField fx:id="passwordTextField" promptText="password" />
      <TextField fx:id="stradaTextField" promptText="strada" />
      <TextField fx:id="numarStradaTextField" promptText="numarStrada" />
      <TextField fx:id="telefonTextField" promptText="telefon" />
      <Label text="Oras" />
      <ComboBox fx:id="orasComboBox" prefWidth="150.0" />
      <Button mnemonicParsing="false" onAction="#registerNewClient" text="Register" />
      <Label text="Username" />
      <ComboBox fx:id="usernameComboBox" onMouseReleased="#login" prefWidth="150.0" />
      <Button fx:id="loginButton" mnemonicParsing="false" onAction="#login" text="Login" />
   </children>
</VBox>
